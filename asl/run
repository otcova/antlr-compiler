#!/bin/bash
set -e

make antlr
make

for i in {1..9}; do
    asl="../examples/jp_chkt_0$i.asl"
    err="../examples/jp_chkt_0$i.err"
    delta "$err" <(./asl "$asl" 2>&1 | sed '/There are semantic errors: no code generated./d')
done

for i in {10..20}; do
    asl="../examples/jp_chkt_$i.asl"
    err="../examples/jp_chkt_$i.err"
    delta "$err" <(./asl "$asl" 2>&1 | sed '/There are semantic errors: no code generated./d')
done